TEST4 +
    START
    ENTRY Go
    EXTRN First, Last, LengR, LengW, Prout, Proutm
    EMPTY Pus, Em1, Em2, Em3

Go      = <Prspe /Pus/ 'as01a432l7l' /Em3/> +
        <Dal>

Bspe
    S   '2134' /Pus/

*      prowerka  specifikacii
Prspe
    E.2 S(/Em1/ /Em2/ /Em3/).1 +
        = <Prout ' symbol-/em/' S.1> <Prspe E.2>
    S(:Bspe:).1 E.k +
        = <Prout ' symbol-1234p=' S.1> <Prspe E.k>
    R E.h S('l7').1 +
        = <Prout ' symbol-l7=' S.1> <Prspe E.h>
    S.s E.k +
        = <Prout '  symbol =' S.s> <Prspe E.k>
        =

Dal     = <Prln /0/ /1/ '345678'> <Prln (/0/ /1/) '345678'> +
        <Prfe /3/ '1234567'> <Prla /3/ '123456'> <Prfe /1/ /2/> +
        <Prla /3/ /1/ /2/ /3/ '567'> +
*  neot
        <Prfe '123'> <Prla '123'>

Prfe
    E.1 = <Prow /First/ E.1>

Prla
    E.1  = <Prow /Last/ E.1>

Prow
    S.1 E.2 +
        = <Proutm S.1 ', whod=' E.2 '  wyh=' < S.1 E.2>>
*
*
Prln
    E.1 = <Prout ' lengr,lengw: whod=' E.1 '  wyh-r=' <LengR E.1> +
        '  wyh-w=' <LengW E.1>>
                                 
    END

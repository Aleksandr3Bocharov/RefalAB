TEST5 +
    START

    ENTRY Go
    EXTRN Prout, Numb, Symb

    IMPL

Go      = <P <Numb '-1000'>> +
        <P <Numb>> +
        <P <Numb '+'>> +
        <P <Numb '-'>> +
        <P <Numb '000000000000000000000000000000001'>> +
        <P <Numb '-000000000000000000000000000000001'>> +
        <P <Numb '+000000000000000000000000000000001'>> +
        <P <Numb '1000000000'>> +
        <P <Numb '4294967295'>> +
        <P <Numb '+15'>> +
        <Prout '0:' <Symb>> +
        <Prout '0:' <Symb '+'>> +
        <Prout '0:' <Symb '-'>> +
        <Prout '0:' <Symb 0>> +
        <Prout '1:' <Symb 0 1>> +
        <Prout '1:' <Symb 0 0 0 0 1>> +
        <Prout '-1:' <Symb '-' 0 0 0 0 1>> +
        <Prout '1:' <Symb '+' 0 0 0 0 1>> +
        <Prout <Symb 4294967295>> +
*  neoty:
        <Numb '4294967296'> +
        <Numb '-4294967296'> +
        <Symb 1 0> +
        <Symb '-' 1 0> 

P   E.N = <Prout E.N ' : ' <Symb E.N>>

    END
